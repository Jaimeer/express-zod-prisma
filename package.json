{
  "name": "express-zod-prisma",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "test": "turbo run test",
    "coverage": "turbo run coverage",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "esbuild": "^0.17.10",
    "esbuild-register": "^3.4.2",
    "eslint-config-custom": "workspace:*",
    "nodemon": "^2.0.20",
    "prettier": "latest",
    "turbo": "latest",
    "vitest": "^0.28.5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@7.27.1"
}